{
    "title": "RAG Chatbot API Metrics",
    "panels": [
      {
        "type": "graph",
        "title": "Requests per Endpoint",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "sum by(path) (rate(http_requests_total[5m]))",
            "legendFormat": "{{path}}",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 }
      },
      {
        "type": "graph",
        "title": "p95 Latency (s)",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_latency_seconds_bucket[5m])) by (le, path))",
            "legendFormat": "{{path}} p95",
            "refId": "B"
          }
        ],
        "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 }
      }
    ],
    "schemaVersion": 16,
    "version": 1
  }
  